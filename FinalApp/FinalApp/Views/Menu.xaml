<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FinalApp.Views.Menu"
             Title="Menú">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="btnInsertPaciente" Text="Insertar Paciente" Clicked="BtnInsertar_Clicked" />
            <Button x:Name="btnSelectPaciente" Text="Ver Pacientes" Clicked="BtnSeleccionar_Clicked" />
            <Button x:Name="btnInsertConsulta" Text="Insertar Consulta" Clicked="BtnConsulta_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>